<div class="container" style="width: 1140px;  justify-content: center; 
    margin: 0 auto; padding: 30px 15px 25px; background: #fff;
    border-radius: 0px; position: static;">
    <article class="page">
        <div style="display: flex;">

            <nav class="main-nav">
                <ul class="main-nav-ul">
                    <li class="menu-left_item lv1"><a href="/user/student/myInbox"><span class="nav-text lv1"> My
                                Inbox</span><span class="sub-arrow"></span></a></li>

                    <li class="menu-left_item lv1"><a class="active" id="lv1_selected"
                            href="/user/student/myApplication">My
                            Application<span class="sub-arrow-selected"></span></a>
                        <ul>
                            <li class="menu-left_item lv2"><a href="/forms/personalParticulars">Personal Particulars</a>
                            </li>
                            <li class="menu-left_item lv2"><a class="active" id="lv2_selected"
                                    href="/forms/academicProfile">Academic Profile</a></li>
                            <li class="menu-left_item lv2"><a href="/forms/academicQualification">Academic
                                    Qualification</a></li>
                            <li class="menu-left_item lv2"><a href="/forms/programmePreference">Programme Preference</a>
                            </li>
                            <li class="menu-left_item lv2"><a href="#">Prize and Awards</a></li>
                            <li class="menu-left_item lv2"><a href="#">Extra-curricular Activities</a></li>
                            <li class="menu-left_item lv2"><a href="/forms/academicReference">Academic Reference</a>
                            </li>
                            <li class="menu-left_item lv2"><a href="/forms/OtherInformation">Other Information</a></li>
                        </ul>
                    </li>
                    <li class="menu-left_item lv1"><a href="/user/student/applicationStatus"><span
                                class="nav-text lv1">Application Status</span><span class="sub-arrow"></span></a>
                    </li>
                    <li class="menu-left_item lv1"><a href="/user/student/myProfile"><span class="nav-text lv1"> My
                                Profile</span><span class="sub-arrow"></span></a>
                    </li>
                </ul>
            </nav>

            <form id="academicProfile" action="/forms/academicProfile" method="POST">

                <div class="main-content" style="margin-left: 40px">
                    <h3 style="color: #333333;
                            font-size: 24px;
                            font-weight: bold;
                            margin-bottom: 5px;">Academic Profile</h3>

                    <div class="container" style="background: #C8D8CC; margin-top: 15px; width: 800px; padding-top: 1px;
                margin-left: 0px ">
                        <p>Years of Schooling</p>
                    </div>

                    <% var year=new Date().getFullYear(); %>
                        <p style=" width: 800px; color: #666666;">Please fill in the years of schooling you have
                            completed/ will complete at different education levels by August 31, <%= year %>. This
                                should take
                                into account only your regular school. Short courses / summer courses / repeated studies
                                should
                                not be counted in this calculation. If any of the level of study is not applicable,
                                please enter
                                "0"
                        </p>

                        <div class="row" style="width: 810px;">
                            <div id="div01" class="form-group col-xl-12 col-12">
                                <div class="row" style="margin-left: 0px ;">
                                    <label class="col-form-label"><span>Primary Education : </span><span
                                            style="color: #F16C6D;">* &nbsp;</span></label>
                                    <input type="number" min="0" class="form-control input-lg"
                                        name="AcademicProfile[primaryYrs]" style="width: 300px" <%
                                        if(req.session.academicProfileData !=null &&
                                        req.session.academicProfileData.AcademicProfile.primaryYrs !=null) { %>
                                    value="<%=req.session.academicProfileData.AcademicProfile.primaryYrs%>" <% } %>
                                            required>
                                            <label class="col-form-label">&nbsp; years full-time study or its
                                                equivalent</label>
                                </div>
                            </div>


                            <div id="div02" class="form-group col-xl-12 col-12">
                                <div class="row" style="margin-left: 0px ;">
                                    <label class="col-form-label"><span>Secondary Education : </span><span
                                            style="color: #F16C6D;">* &nbsp;</span></label>
                                    <input type="number" min="0" class="form-control input-lg"
                                        name="AcademicProfile[secondaryYrs]" style="width: 300px" <%
                                        if(req.session.academicProfileData !=null &&
                                        req.session.academicProfileData.AcademicProfile.secondaryYrs !=null) { %>
                                    value="<%=req.session.academicProfileData.AcademicProfile.secondaryYrs%>" <% } %>
                                            required>
                                            <label class="col-form-label">&nbsp; years full-time study or its
                                                equivalent</label>
                                </div>
                            </div>

                            <div id="div03" class="form-group col-xl-12 col-12">
                                <div class="row" style="margin-left: 0px ;">
                                    <label class="col-form-label"><span>Post Secondary Education : </span><span
                                            style="color: #F16C6D;">* &nbsp;</span></label>
                                    <input type="number" min="0" class="form-control input-lg"
                                        name="AcademicProfile[postYrs]" style="width: 300px" <%
                                        if(req.session.academicProfileData !=null &&
                                        req.session.academicProfileData.AcademicProfile.postYrs !=null) { %>
                                    value="<%=req.session.academicProfileData.AcademicProfile.postYrs%>" <% } %>
                                            required>
                                            <label class="col-form-label">&nbsp; years full-time study or its
                                                equivalent</label>
                                </div>
                            </div>

                        </div>

                        <div class="container" style="background: #C8D8CC; margin-top: 15px; width: 800px; padding-top: 1px;
                    margin-left: 0px ">
                            <p>Post Secondary Education</p>
                        </div>

                        <!-- 1st Post Secondary Education Record -->
                        <div class="row" id="postSecRecord1" style="max-width: 810px;">
                            <table style="color: #333333; margin-left: 20px; padding-top: 0px; max-width: 800px; ">
                                <tr>
                                    <th>Country/ Region</th>
                                    <td class="table-content">
                                        <select class="form-control" id="postCountry1"
                                            name="AcademicProfile[postCountry1]"
                                            style="height: 20px; padding: 0px; width: 410px">
                                        </select>
                                    </td>
                                </tr>

                                <tr>
                                    <th>Name of Institution</th>
                                    <td class="table-content">
                                        <input type="text" class="form-control" id="postInstitution1"
                                            name="AcademicProfile[postInstitution1]" style="height: 21px; padding: 4px;"
                                            <% if(req.session.academicProfileData !=null &&
                                            req.session.academicProfileData.AcademicProfile.postInstitution1 !=null) {
                                            %>
                                        value="<%=req.session.academicProfileData.AcademicProfile.postInstitution1%>" <%
                                                }%>
                                                />
                                    </td>
                                </tr>

                                <tr>
                                    <th>Programme Title</th>
                                    <td class="table-content">
                                        <input type="text" class="form-control" id="postProgramTitle1"
                                            name="AcademicProfile[postProgramTitle1]"
                                            style="height: 21px; padding: 4px;" <% if(req.session.academicProfileData
                                            !=null && req.session.academicProfileData.AcademicProfile.postProgramTitle1
                                            !=null) { %>
                                        value="<%=req.session.academicProfileData.AcademicProfile.postProgramTitle1%>"
                                            <% } %>
                                                />
                                    </td>
                                </tr>

                                <tr>
                                    <th>Study Status</th>
                                    <td class="table-content">
                                        <select class="form-control" id="postStudyStatus1"
                                            name="AcademicProfile[postStudyStatus1]"
                                            style="height: 20px; padding: 0px; width: 150px">
                                            <option value="" selected="true" disabled="disabled">-Select-</option>
                                            <option value="graduated" <% if(req.session.academicProfileData !=null &&
                                                req.session.academicProfileData.AcademicProfile.postStudyStatus1 !=null)
                                                { %>
                                                <%=(req.session.academicProfileData.AcademicProfile.postStudyStatus1=='graduated'
                                                    )? 'selected' :'' %>
                                                    <% } %>
                                                        >Graduated
                                            </option>

                                            <option value="final year" <% if(req.session.academicProfileData !=null &&
                                                req.session.academicProfileData.AcademicProfile.postStudyStatus1 !=null)
                                                { %>
                                                <%=(req.session.academicProfileData.AcademicProfile.postStudyStatus1== 'final year'
                                                    )? 'selected' :'' %>
                                                    <% } %>
                                                        >Final year
                                            </option>

                                            <option value="non-final year" <% if(req.session.academicProfileData !=null
                                                && req.session.academicProfileData.AcademicProfile.postStudyStatus1
                                                !=null) { %>
                                                <%=(req.session.academicProfileData.AcademicProfile.postStudyStatus1== 'non-final year'
                                                    )? 'selected' :'' %>
                                                    <% } %>>Non-final year
                                            </option>

                                            <option value="withdrew" <% if(req.session.academicProfileData !=null &&
                                                req.session.academicProfileData.AcademicProfile.postStudyStatus1 !=null)
                                                { %>
                                                <%=(req.session.academicProfileData.AcademicProfile.postStudyStatus1=='withdrew'
                                                    )? 'selected' :'' %>
                                                    <% } %>>Withdrew
                                            </option>

                                            <option value="deferred" <% if(req.session.academicProfileData !=null &&
                                                req.session.academicProfileData.AcademicProfile.postStudyStatus1 !=null)
                                                { %>
                                                <%=(req.session.academicProfileData.AcademicProfile.postStudyStatus1=='deferred'
                                                    )? 'selected' :'' %>
                                                    <% } %>>Deferred
                                            </option>
                                        </select>
                                    </td>
                                </tr>

                                <tr>
                                    <th>Study Mode</th>
                                    <td class="table-content">
                                        <div class="select_join">
                                            <select class="form-control" id="postStudyMode1"
                                                name="AcademicProfile[postStudyMode1]"
                                                style="height: 20px; padding: 0px; width: 150px">
                                                <option value="" selected="true" disabled="disabled">-Select-</option>
                                                <option value="full-time" <% if(req.session.academicProfileData !=null
                                                    && req.session.academicProfileData.AcademicProfile.postStudyMode1
                                                    !=null) { %>
                                                    <%=(req.session.academicProfileData.AcademicProfile.postStudyMode1=='full-time'
                                                        )? 'selected' :'' %>
                                                        <% } %>
                                                            >Full-time
                                                </option>
                                                <option value="part-time" <% if(req.session.academicProfileData !=null
                                                    && req.session.academicProfileData.AcademicProfile.postStudyMode1
                                                    !=null) { %>
                                                    <%=(req.session.academicProfileData.AcademicProfile.postStudyMode1=='part-time'
                                                        )? 'selected' :'' %>
                                                        <% } %>>Part-time
                                                </option>
                                            </select>
                                        </div>
                                    </td>
                                </tr>

                                <tr>
                                    <th>Latest Cumulative GPA</th>
                                    <td class="table-content">
                                        <div class="row" style="margin-left: 0px ;">
                                            <input type="text" class="form-control input-lg" id="postGPA1"
                                                name="AcademicProfile[postGPA1]" pattern="^\d*(\.\d{0,2})?$"
                                                style="height: 21px; padding: 4px; width: 75px;" placeholder="e.g. 3.33"
                                                <% if(req.session.academicProfileData !=null &&
                                                req.session.academicProfileData.AcademicProfile.postGPA1 !=null) { %>
                                            value="<%=req.session.academicProfileData.AcademicProfile.postGPA1%>" <% }
                                                    %>
                                                    />
                                                    <label class="col-form-label"
                                                        style="padding: 0px; margin: 0px;"><span
                                                            style="font-size: 14px;">&nbsp;out of maximum
                                                            cGPA&nbsp;</span></label>
                                                    <input type="text" class="form-control input-lg" id="postMaxGPA1"
                                                        name="AcademicProfile[postMaxGPA1]"
                                                        style="height: 21px; padding: 4px; width: 65px;"
                                                        placeholder="e.g. 4.0" <% if(req.session.academicProfileData
                                                        !=null &&
                                                        req.session.academicProfileData.AcademicProfile.postMaxGPA1
                                                        !=null) {%>
                                                    value="<%=req.session.academicProfileData.AcademicProfile.postMaxGPA1%>"
                                                        <% } %>
                                                            />
                                        </div>
                                    </td>
                                </tr>

                                <tr>
                                    <th>Date of Admission</th>
                                    <td class="table-content">
                                        <input type="date" class="form-control" id="postPeriodFrom1"
                                            name="AcademicProfile[postPeriodFrom1]"
                                            style="height: 21px; width: 150px; padding: 4px;" <%
                                            if(req.session.academicProfileData !=null &&
                                            req.session.academicProfileData.AcademicProfile.postPeriodFrom1 !=null) { %>
                                        value="<%=req.session.academicProfileData.AcademicProfile.postPeriodFrom1%>" <%
                                                } %>
                                                />
                                    </td>
                                </tr>

                                <tr>
                                    <th>Date / Expected Date of Completion</th>
                                    <td class="table-content">
                                        <input type="date" class="form-control" id="postPeriodTo1"
                                            name="AcademicProfile[postPeriodTo1]" placeholder="From"
                                            style="height: 21px; width: 150px; padding: 4px;" <%
                                            if(req.session.academicProfileData !=null &&
                                            req.session.academicProfileData.AcademicProfile.postPeriodTo1 !=null) { %>
                                        value="<%=req.session.academicProfileData.AcademicProfile.postPeriodTo1%>" <% }
                                                %>
                                                />
                                    </td>
                                </tr>

                                <tr>
                                    <th>Qualification Obtained / to be Obtained</th>
                                    <td class="table-content">
                                        <div class="select_join">
                                            <select class="form-control" id="postQualification1"
                                                name="AcademicProfile[postQualification1]"
                                                style="height: 20px; padding: 0px; width: 280px">
                                            </select>
                                        </div>
                                    </td>
                                </tr>
                                <!-- <tr>
                                    <th>Action</th>
                                    <td class="table-content">
                                        <div class="row" style="margin-left: 0px; clear: both">
                                 
                                            <button type="submit" id="deleteBtn" class="btn btn-primary"
                                                onclick="clearFormInput()" formaction="/forms/academicProfile/save"><i
                                                    class="fa fa-trash-o" aria-hidden="true"></i>
                                                Clear</button>
                                        </div>
                                    </td>
                                </tr> -->
                            </table>
                        </div>
                        <!-- End of 1st Record -->

                        <div class="container" style="background: #C8D8CC; margin-top: 15px; width: 800px; padding-top: 1px;
                    margin-left: 0px ">
                            <p>Secondary Education</p>
                        </div>

                        <!-- 1st Secondary Education Record -->
                        <div class="row" id="SecRecord1" style="max-width: 810px;">
                            <table style="color: #333333; margin-left: 20px; padding-top: 0px; max-width: 800px; ">
                                <tr>
                                    <th>Country/ Region</th>
                                    <td class="table-content">
                                        <select class="form-control" id="secCountry1"
                                            name="AcademicProfile[secCountry1]"
                                            style="height: 20px; padding: 0px; width: 410px" required>
                                        </select>
                                    </td>
                                </tr>

                                <tr>
                                    <th>Name of School</th>
                                    <td class="table-content">
                                        <input type="text" class="form-control" name="AcademicProfile[secSchoolName1]"
                                            style="height: 21px; padding: 4px;" <% if(req.session.academicProfileData
                                            !=null && req.session.academicProfileData.AcademicProfile.secSchoolName1
                                            !=null) { %>
                                        value="<%=req.session.academicProfileData.AcademicProfile.secSchoolName1%>" <%
                                                }%>
                                                required/>
                                    </td>
                                </tr>

                                <tr>
                                    <th>Curriculum</th>
                                    <td class="table-content">
                                        <select class="form-control" id="secCurriculum1"
                                            name="AcademicProfile[secCurriculum1]"
                                            style="height: 20px; padding: 0px; width: 410px" required>
                                        </select>
                                    </td>
                                </tr>

                                <tr>
                                    <th>Date of Admission</th>
                                    <td class="table-content">
                                        <input type="date" class="form-control" name="AcademicProfile[secPeriodFrom1]"
                                            style="height: 21px; width: 150px; padding: 4px;" <%
                                            if(req.session.academicProfileData !=null &&
                                            req.session.academicProfileData.AcademicProfile.secPeriodFrom1 !=null) { %>
                                        value="<%=req.session.academicProfileData.AcademicProfile.secPeriodFrom1%>" <% }
                                                %>
                                                required/>
                                    </td>
                                </tr>

                                <tr>
                                    <th>Date / Expected Date of Completion</th>
                                    <td class="table-content">
                                        <input type="date" class="form-control" name="AcademicProfile[secPeriodTo1]"
                                            placeholder="From" style="height: 21px; width: 150px; padding: 4px;" <%
                                            if(req.session.academicProfileData !=null &&
                                            req.session.academicProfileData.AcademicProfile.secPeriodTo1 !=null) { %>
                                        value="<%=req.session.academicProfileData.AcademicProfile.secPeriodTo1%>" <% }
                                                %>
                                                required/>
                                    </td>
                                </tr>

                                <tr>
                                    <% var year=new Date().getFullYear(); %>
                                        <th>Level Attained (by <%= year %>-08-31)</th>
                                        <td class="table-content">
                                           
                                                <select class="form-control" id="secLevelAttained1"
                                                    name="AcademicProfile[secLevelAttained1]"
                                                    style="height: 20px; padding: 0px; width: 280px" required>
                                                </select>
                                            
                                        </td>
                                </tr>
                            </table>
                        </div>
                        <!-- End of 1st Secondary Education Record -->

                        <div>
                            <br>
                            <br>
                            <!-- Save -->
                            <button type="submit" id="saveBtn" class="btn btn-primary"
                                formaction="/forms/academicProfile/save"><b>Save</b></button>

                            <!-- Continue -->
                            <button type="submit" id="saveContinue_Btn" class="btn btn-primary">
                                <b>Save and Continue</b></button>
                        </div>
                </div>
            </form>
        </div>
    </article>
</div>

<script>
    (function () {

