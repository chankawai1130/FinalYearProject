        font-family: "Helvetica Neue", Helvetica, Arial;
    }

    .clearfix::after {
        content: "";
        clear: both;
        display: table;
    }

    @media screen and (max-width: 300px) {

        .cancelbtn,
        .deletebtn .confirmbtn {
            width: 100%;
