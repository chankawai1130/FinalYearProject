    .sendBtn:focus {
        background: #C4C491;
    }

    .confirmbtn:focus,
    .deletebtn:focus {
        color: black;
    }

    .cancelbtn,
    .deletebtn,
    .confirmbtn {
        float: left;
        width: 50%;
    }

    .cancelbtn {
        background-color: #ccc;
        color: black;
    }

    .deletebtn {
        background-color: #F16C6D;
    }

    .confirmbtn {
        background-color: #869A85;
    }

    .modal-container {
        padding: 16px;
        text-align: center;
    }

    .modal {
        display: none;
        position: fixed;
        z-index: 3;
        left: 0;
        top: 0;
        width: 100%;
        height: 100%;
        overflow: auto;
        background-color: rgba(8, 8, 8, 0.5);
        padding-top: 50px;
    }

    .modal-content {
        background-color: #fefefe;
        margin: 5% auto 15% auto;
        border: 1px solid #888;
        width: 80%;
        font-family: "Helvetica Neue", Helvetica, Arial;
