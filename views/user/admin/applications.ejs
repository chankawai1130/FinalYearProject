<script>
    function compareApplication(tableID) {
        var table = document.getElementById(tableID);
        var rowCount = table.rows.length;
        var selectedRows = "";
        for (var i = 0; i < rowCount; i++) {
            var row = table.rows[i];
            var chkbox = row.cells[1];
            if (chkbox != null && chkbox.checked == true) {
                selectedRows = selectedRows + ":" + row.cells[2].innerText;
            }
        }
        document.getElementById("demo").innerHTML = selectedRows;

        document.getElementById("sApplication").value = selectedRows;

        // toLineBreak();
    }
</script>

