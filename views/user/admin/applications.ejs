    }

    .sendReminder {
        background-color: #FEFAFF;
        color: #666666;
        border-radius: 2px;
        width: 95px;
        margin-bottom: 10px;
        margin-right: 5px;
        font-size: 14px;
        padding: 4px 9px 4px;
    }

    .sendReminder:hover {
        background-color: #F9F5FA;
    }


    #approveBtn {
        width: 135px;
        background-color: #869A85;
        color: #fff;
        font-weight: bold;
        margin-bottom: 5px;
        opacity: 0.7;
    }

    #disapproveBtn {
        width: 135px;
        font-weight: bold;
        /* background-color: #F34541; */
        /* background-color: #F16C6D; */
        background-color: #E97D7D;
        color: #fff;
        opacity: 0.7;
    }

    #disapproveBtn:hover {
        opacity: 0.8;
    }

    #approveBtn:hover {
        opacity: 0.8;
    }

    #statusContainer {
        margin-right: 30px;
        padding: 6px 13px;
        /* background-color: #89837A; */
        text-align: center;
        display: inline;
        border-radius: 20px;
        /* background-image: linear-gradient(#a39c92, #b6b0a6); */
        /* color: #fff; */
        background-color: #cfc9be;
        color: #52504d;
        font-weight: 500;
        opacity: 0.9;
        text-shadow:
            0px 0px 3px rgba(255, 255, 255, 0.3),
            0px 0px 0px rgba(8, 8, 8, 0);
    }

    .hidden_row {
        display: none;
    }

    #declineOfferBtn {
        background-color: #f5f5f5;
        color: #777777;
        width: 200px;
        margin-top: 0px;
        margin-bottom: 10px;
        font-size: 16px;
        padding: 4px 9px 4px;
        opacity: 0.8;
    }
</style>

