<div class="wrapper" style="width: 1310px; margin: 0 auto;">
    <div class="container-fluid" style="justify-content: center; 
    padding: 30px 15px 25px; background: #fff;
    border-radius: 0px; position: static;">

        <div style="display: flex;">

            <div class="main-content" style="margin-left: 40px;">
                <h3 style="color: #333333;
            font-size: 32px;
            font-weight: bold;
            margin-bottom: 20px;">Applications
                </h3>

                <div>
                    <div style="margin-bottom: 10px; float: left">
                        <span><b><a href="/user/admin/applications/all/search" style="color: #333333;">All </a></b></span><span style="color: #9b9696;">(<%= allNum%>)
                                |</span>
                        <span style="padding-left: 3px; font-weight: 600;"><a
                                href="/user/admin/applications/approved/search" style="color:#6d8c9c;">
                                Approved
                            </a></span><span style="color: #9b9696;">(<%= approvedNum%>) |</span>
                        <span style="padding-left: 3px; font-weight: 600;"><a
                                href="/user/admin/applications/disapproved/search" style="color:#6d8c9c;">
                                Disapproved
                            </a></span><span style="color: #9b9696;">(<%= disapprovedNum%>) |</span>
                    </div>

                    <div style="float: right;">
                        <div class="form-inline">
                            <form action="/user/admin/applications/all/search" method="GET">
                                <input type="text" id="searchField" class="form-control input-lg"
                                    value="<%= req.query.sApplication %>" name="sApplication"
                                    placeholder="Search applications">
                                <button type="submit" id="searchBtn" class="form-control btn-lg">Search</button>
                            </form>
                        </div>
                    </div>
                </div>

                <div class="row" style="margin-left: 0px; clear: both">

                    <div class="row" style="margin-left: 0px; clear: both">
                        <button type="button" class="form-control btn-lg" id="sendEmail"
                            onclick="document.getElementById('customerizeEmailModal').style.display='block'">Send
                            Email</button>
                    </div>
                    <p id="blank"><br></p>

                    <!-- confirm box -->
                    <div id="customerizeEmailModal" class="modal">
                        <form action="/user/admin/sendEmail" method="POST">
                            <div class="interviewModal-content">
                                <div style="padding: 16px 16px 16px 30px;">
                                    <!-- modal-header -->
                                    <span class="close"
                                        onclick="document.getElementById('customerizeEmailModal').style.display='none'">&times;</span>
                                    <h3 style="color: black;
                                        font-size: 24px;
                                        font-weight: 500;
                                        margin-bottom: 20px;
                                        margin-top: 15px;
                                        padding-left: 27px;">New Message
                                    </h3>

                                    <div class="modal-body" style="color: #444444;">
                                        <!-- modal-body -->
                                        <div id="div01" class="form-group col-xl-12 col-12">
                                            <div class="row" style="margin-left: 0px;">
                                                <label class="col-form-label"
                                                    style="padding-right: 30px;"><span>Recipient
                                                    </span></label>
                                                <input type="text" class="form-control input-lg" name="User[recipient]"
                                                    style="width: 600px; margin-left: 4px; font-size: 14px;">
                                            </div>
                                        </div>

                                        <div id="div02" class="form-group col-xl-12 col-12">
                                            <div class="row" style="margin-left: 0px;">
                                                <label class="col-form-label" style="padding-right: 43px;"><span>Subject
                                                    </span></label>
                                                <input type="text" class="form-control input-lg" name="User[subject]"
                                                    style="width: 600px; margin-left: 4px; font-size: 14px;">
                                            </div>
                                        </div>

                                        <div id="div03" class="form-group col-xl-12 col-12">
                                            <div class="row" style="margin-left: 0px;">
                                                <label class="col-form-label"
                                                    style="padding-right: 43px;"><span>Content</span></label>
                                                <textarea rows="10" cols="50" id="content" class="form-control input-lg"
                                                    name="User[content]" style="width: 600px; font-size: 14px;"
                                                    required></textarea>
                                            </div>
                                        </div>

                                        <input type="hidden" id="sApplication" name="sApplication">
                                        
                                        <div>
                                            <button type="submit" onclick="compareApplication('dataTable')"
                                                class="sendBtn modalBtn" id="sendBtn1">
                                                <i class="fa fa-envelope-o" aria-hidden="true"></i>&nbsp
                                                Send</button>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                    <!-- end -->

                </div>

                <!-- <div class="row" style="margin-left: 0px; clear: both">
                    
                    <button class="form-control btn-lg" id="sendEmail">Send Email</button>
                    
                    <button class="form-control btn-lg" id="scheduleInterview">Schedule Interview</button>
                    <p id="blank"><br></p>
                </div> -->


                <table id="dataTable" style="width: 1190px;">

                    <!-- table header -->
                    <tr style="border-bottom: 1px solid #f1f1f1; color: #666666; font-weight: 500;">
                        <td></td>
                        <td><input type="checkbox" id="select-all"></td>
                        <td>Application No.</td>
                        <td>Applicant Name</td>
                        <td>Applicant Email</td>
                        <td style="text-align: center;  padding-right: 36px;">Status</td>
                        <!-- <td>Scheduled Interview Date/ Time</td> -->
                        <td>Action</td>
                    </tr>
                    <!-- end -->
                    <% applications.forEach( function(model) { %>
                        <tr style="height: 70px; border-bottom: 1px solid #f1f1f1;">
                            <td width="2%" style="padding-left: 30px; padding-right: 10px; color:#5A6872;">
                                <button class="collapsible" id="dropDownBtn"
                                    onclick="showHideRow('hidden_row<%= model.id %>');"></button>
                            </td>
                            <td width="2%"><input type="checkbox" id="checkbox<% model.id %>" name="select-applications"
                                    onclick="showContent();">
                            </td>
                            <td width="14%"><a href="/user/admin/applications/details/<%= model.id%>"
                                    style="color:#4677B6; font-weight: 600;">
                                    <%= model.idCode%>
                                </a>
                            </td>
                            <td width="16%">
                                <%= model.Surname%>
                                    <%= model.GivenName%>
                            </td>
                            <td width="18%" style="color:#3D70B3;">
                                <%= model.Email %>
                            </td>
                            <!-- 13/01/2021 &nbsp 01:59 AM-->
                            <td width="16%" style="text-align: center; ">
                                <div class="container" id="statusContainer">
                                    <% if (model.status=="submitted" ) { %>
                                        <span>
                                            Submitted
                                        </span>
                                        <% } else if (model.status=="approved" ) { %>
                                            <span>
                                                Approved
                                            </span>
                                            <% } else if (model.status=="disapproved" ) { %>
                                                <span>
                                                    Disapproved
                                                </span>
                                                <% } %>
                                </div>
                            </td>

                            <td width="15%">
                                <% if (model.status=='submitted' ) { %>
                                    <!-- approve button -->
                                    <button class="form-control" id="approveBtn"
                                        onclick="document.getElementById('approveModal').style.display='block'"><i
                                            class="fa fa-check" aria-hidden="true"></i> APPROVE</button>

                                    <!-- confirm box -->
                                    <div id="approveModal" class="modal">
                                        <div class="modal-content">
                                            <div class="modal-container">
                                                <!-- modal-header -->
                                                <h1>Approve Application?</h1>
                                                <h4 style="margin-bottom: 20px;">(Application No.: <%= model.idCode%>)
                                                </h4>
                                                <!-- modal-body -->
                                                <p>Are you sure you want to approve this application?</p>
                                                <div class="clearfix">
                                                    <button type="button" class="cancelbtn modalBtn"
                                                        onclick="document.getElementById('approveModal').style.display='none'">
                                                        Cancel</button>
                                                    <button type="button" class="confirmbtn modalBtn"
                                                        onclick="approveApplication('<%= model.id%>')">Approve</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- end -->

                                    <!-- disapprove button -->
                                    <button class="form-control" id="disapproveBtn"
                                        onclick="document.getElementById('disapproveModal').style.display='block'"><i
                                            class="fa fa-times" aria-hidden="true"></i> DECLINE</button>

                                    <!-- confirm box -->
                                    <div id="disapproveModal" class="modal">
                                        <div class="modal-content">
                                            <div class="modal-container">
                                                <h1>Disapprove Application?</h1>
                                                <h4 style="margin-bottom: 20px;">(Application No.: <%= model.idCode%>)
                                                </h4>
                                                <p>Are you sure you want to disapprove this application?</p>
                                                <div class="clearfix">
                                                    <button type="button" class="cancelbtn modalBtn"
                                                        onclick="document.getElementById('disapproveModal').style.display='none'">
                                                        Cancel</button>
                                                    <button type="button" class="deletebtn modalBtn"
                                                        onclick="disapproveApplication('<%= model.id%>')">
                                                        Disapprove</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- end -->
                                    <% } %>

                                        <% if (model.status=='approved' ) { %>
                                            <div style="width: 120px;">--</div>
                                            <% } %>

                                                <% if (model.status=='disapproved' ) { %>
                                                    <div style="width: 120px;">--</div>
                                                    <% } %>
                            </td>
                            <!-- <td><a style="color:#0089CB;" href="#">Approve</a> | <a style="color:#0089CB;" href="#">Disapprove</a></td> -->
                        </tr>

                        <tr id="hidden_row<%= model.id %>" class="hidden_row">
                            <td colspan=7 style="border-bottom: 1px solid #f1f1f1;">
                                <table>
                                    <tr style="color: #777777;">
                                        <td width="60%"
                                            style="padding-top: 30px; padding-bottom: 0px; padding-left: 115px;">1st
                                            Programme Choice</td>
                                        <td style="padding-top: 30px;">Scheduled Interview Date/ Time</td>
                                    </tr>
                                    <tr style="color: #333333;">
                                        <td style="padding-top: 0px; padding-bottom: 20px; padding-left: 115px;">
                                            <% if (model.program1 !="" ) {%>

                                                <%= model.program1 %> (<%= model.entryYear1 %>)

                                                        <% } %>
                                        </td>
                                        <td>

                                            <% if (model.haveInterview1=="no" ) { %>
                                                -
                                                <% } else { %>
                                                    <div class="container" id="interviewDate"
                                                        style="display: inline-block;">
                                                        <label class="col-form-label">
                                                            <span style="max-width: 50px; color: #757575;">

                                                                <% if (model.firstInterviewDate==null &&
                                                                    model.firstInterviewTime==null) {%>
                                                                    <span
                                                                        style="color: #a5a4a4; padding-right: 5px;">Select
                                                                        Date / Time</span>
                                                                    <% } else {%>
                                                                        <u>
                                                                            <%= model.firstInterviewDate %> &nbsp; <%=
                                                                                    model.firstInterviewTime %>
                                                                        </u>
                                                                        <% } %>

                                                            </span>
                                                        </label><span class="separator"></span>
                                                        <%var interviewID="int" + model.id + 1%>
                                                            <button class="form-control btn-lg" id="selectDateBtn"
                                                                data-backdrop="static"
                                                                onclick="document.getElementById('selectTimeSlotModal<%= interviewID%>').style.display='block'">
                                                                <i class="fa fa-calendar" aria-hidden="true"></i>
                                                            </button>

                                                            <!-- confirm box -->
                                                            <div id="selectTimeSlotModal<%= interviewID%>"
                                                                class="modal">
                                                                <form
                                                                    action="/applications/firstPreference/scheduleInterview/<%= model.id %>"
                                                                    method="POST">
                                                                    <div class="interviewModal-content">
                                                                        <div style="padding: 16px 16px 16px 30px;">
                                                                            <!-- modal-header -->
                                                                            <span class="close"
                                                                                onclick="document.getElementById('selectTimeSlotModal<%= interviewID%>').style.display='none'">&times;</span>
                                                                            <h3 style="color: black;
                                                                            font-size: 24px;
                                                                            font-weight: 500;
                                                                            margin-bottom: 20px;
                                                                            margin-top: 15px;
                                                                            padding-left: 27px;">Select Interview Time
                                                                                Slot
                                                                            </h3>

                                                                            <div class="modal-body"
                                                                                style="color: #444444;">
                                                                                <!-- modal-body -->
                                                                                <div id="div01"
                                                                                    class="form-group col-xl-12 col-12">
                                                                                    <div class="row"
                                                                                        style="margin-left: 0px;">
                                                                                        <label class="col-form-label"
                                                                                            style="padding-right: 160px;"><span>Date
                                                                                                of
                                                                                                the
                                                                                                Interview</span></label>
                                                                                        <input type="date"
                                                                                            class="form-control input-lg"
                                                                                            value="<%= model.firstInterviewDate %>"
                                                                                            name="User[firstInterviewDate]"
                                                                                            style="width: 350px"
                                                                                            required>
                                                                                    </div>
                                                                                </div>
                                                                                <div id="div02"
                                                                                    class="form-group col-xl-12 col-12">
                                                                                    <div class="row"
                                                                                        style="margin-left: 0px;">
                                                                                        <label class="col-form-label"
                                                                                            style="padding-right: 160px;"><span>Time
                                                                                                of
