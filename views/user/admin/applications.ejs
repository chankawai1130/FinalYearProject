    .fa-calendar {
        /* color: #C0C0C0; */
        color: #7F7F7F;
    }

    .fa-calendar:hover {
        color: #666666;
    }

    .separator {
        content: '';
        display: inline-block;
        background: #e0dfdf;
        margin-left: 16px;
        margin-right: 4px;
        height: 38px;

        vertical-align: top;
