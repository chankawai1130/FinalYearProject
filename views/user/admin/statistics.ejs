        var approvedNum = '<%=approvedNum%>' / '<%=allNum%>' * 100;
        var disapprovedNum = '<%=disapprovedNum%>' / '<%=allNum%>' * 100;
        var pendingNum = '<%=pendingNum%>' / '<%=allNum%>' * 100;
        var chart = new CanvasJS.Chart("chartContainer", {
            title: {
                text: "Application Status",
                fontFamily: "Arial",
                fontWeight: "bold"
            },
            data: [{
                type: "pie",
                legendText: "{indexLabel}",
                startAngle: 240,
                yValueFormatString: "##0.00\"%\"",
                indexLabel: "{label} {y}",
                dataPoints: [
                    { y: approvedNum, label: "Approved (<%=approvedNum%>)", color: "#A4ADB0" },
                    { y: disapprovedNum, label: "Disapproved (<%=disapprovedNum%>)", color: "#7B7366" },
                    { y: pendingNum, label: "Pending (<%=pendingNum%>)", color: "#A29A8E" },

                ]
            }]
        });
        chart.render();
    }

    var acceptedNum = '<%=acceptedNum%>' / '<%=applicationNum%>' * 100;
    var rejectedNum = '<%=rejectedNum%>' / '<%=applicationNum%>' * 100;
    var underConsiderationNum = '<%=underConsiderationNum%>' / '<%=applicationNum%>' * 100;
    var chart = new CanvasJS.Chart("chartContainer2", {
        title: {
            text: "Programme Application Status",
            fontFamily: "Arial",
            fontWeight: "bold"
        },
        data: [{
            type: "pie",
            legendText: "{indexLabel}",
            startAngle: 240,
            yValueFormatString: "##0.00\"%\"",
            indexLabel: "{label} {y}",
            dataPoints: [
                { y: acceptedNum, label: "Accepted (<%=acceptedNum%>)", color: "#A4ADB0" },
                { y: rejectedNum, label: "Declined (<%=rejectedNum%>)", color: "#7B7366" },
                { y: underConsiderationNum, label: "Pending (<%=underConsiderationNum%>)", color: "#A29A8E" },

            ]
        }]
    });
    chart.render();

</script>

